<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.NewPasswordViewController">
   <children>
      <TextField fx:id="txtFieldPassword" layoutX="52.0" layoutY="88.0" prefHeight="25.0" prefWidth="196.0" promptText="Enter new password" />
      <Label layoutX="20.0" layoutY="33.0" text="Your account has not yet been assigned a password" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="52.0" layoutY="68.0" text="New password" />
      <Button layoutX="127.0" layoutY="151.0" mnemonicParsing="false" onAction="#onClickSavePassword" text="Save" />
   </children>
</AnchorPane>
